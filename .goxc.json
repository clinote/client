{
  "AppName": "cn",
  "ConfigVersion": "0.9",
  "ResourcesExclude": "README.md",
  "ArtifactsDest": "build",
  "BuildConstraints": "linux,amd64 darwin,amd64",
  "BuildSettings": {
    "LdFlags": "-s",
    "LdFlagsXVars": {
      "Version": "main.VERSION"
    }
  },
  "Tasks": [
    "interpolate-source",
    "default"
  ],
  "TasksExclude": [
    "rmbin",
    "deb",
    "deb-dev",
    "downloads-page",
    "go-vet",
    "go-test"
  ]
}
